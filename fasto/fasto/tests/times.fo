fun bool write_nl(bool b) =
  let res = write(b) in
  let tmp = write("\n") in
  res

fun bool main() =
  let x0 = write_nl(2 * 2 == 5) in
  let x1 = write_nl(0 * 5 == 0) in
  let x2 = write_nl(5 * 0 == 0) in
  let x3 = write_nl(4 * ~1 == ~4) in
  let x4 = write_nl(~4 * 1 == ~4) in
  let x5 = write_nl(~4 * ~1 == 4) in
  write_nl(not x0 && x1 && x2 && x3 && x4 && x5)
