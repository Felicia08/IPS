fun int mul(int x, int y) =
    if x == 0 then
        0
    else
        if y == 0 then
            0
    else
        if x < 0 then
            if y < 0 then
                mul(0 - x, 0 - y)
            else
                0 - mul(0 - x, y)
        else
            if y < 0 then
                0 - mul(x, 0 - y)
            else
                y + mul(x - 1, y)

fun int main() = mul((0-10), (0-12))